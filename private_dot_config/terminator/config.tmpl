[global_config]
  enabled_plugins = TerminatorThemes, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  suppress_multiple_term_dialog = True
  title_font = Source Code Pro Medium 9
  title_use_system_font = False
[keybindings]
  close_term = <Primary><Shift>w
  close_window = <Primary><Shift><Alt>q
  split_horiz = <Primary><Shift>k
  split_vert = <Primary><Shift>l
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = Spacedust
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
